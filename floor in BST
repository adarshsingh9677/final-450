void findfloor(Node *root, int k,int &floorval)
	{
	    while(root!=NULL)
	    {
	        if(root->data==k)
	        {
	            floorval=root->data;
	            return;
	        }
	        if(k<root->data)
            {
                root=root->left;
            }
            else
            {
                floorval=root->data;
                root=root->right;
            }
	    }
	}
